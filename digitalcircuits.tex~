\chapter{Math Beyond Multivariable Calc}
\section{Numerically-Stable Quadratic Formula}
% For any quadratic
% \begin{equation}
% \label{eq:45}
% 1 + a_{1} x + a_{2} x^{2} = 0,
% \end{equation}
% define a $Q$ factor,
% \begin{equation}
% \label{eq:46}
% Q^{2} = \frac{a_{2}}{a_{1}^{2}}
% \end{equation}
% and plug it into
% \begin{equation}
% \label{eq:47}
% F = \frac{1}{2} \left( 1 + \sqrt{1 - 4 Q^{2}} \right).
% \end{equation}
% The solution is $F$. This equation is more numerically stable than the usual quadratic formula.
% Taken from Middlebrook's Technical Therapy course.
